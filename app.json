        {
          "origin": "https://buez.expo.app"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.buez.stripe",
          "publishableKey": "pk_test_51LtcrdFQprReXlK6tCsDooVIYLhyMgc0AkW6SUcBGshGUN0rfZCmfc7riWPJq8QtO2Xe6WzuPLBM4qKInsX6txGo00MzVdCD03",
